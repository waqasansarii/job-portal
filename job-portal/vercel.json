{
    "builds":[
        {
            "src":"main/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runTime":"python3.9"
            }
        },
        {
            "src":"build.sh",
            "use":"@vercel/static-build"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"main/wsgi.py"
        }
    ]
}